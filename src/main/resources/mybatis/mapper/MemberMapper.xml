<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "Mapper" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.themembers.common.mapper.MemberMapper">
	<select id="findAll" resultType="member">
		SELECT * FROM MEMBER
	</select>
	
	<select id="findAlltest" resultType="hashmap">
		SELECT * FROM MEMBER
	</select>
	
	<insert id="create" parameterType="map">
		INSERT INTO MEMBER VALUES(DEFAULT,#{memName},#{memId},#{memPwd},#{memTel},#{memGender},#{memBirth},DEFAULT,DEFAULT,"Y",DEFAULT,#{updUser},#{mailYn},#{snsYn},#{memNick})
	</insert>
	
	<insert id="createTest" parameterType="com.themembers.common.dto.MemberDTO">
		INSERT INTO MEMBER VALUES(DEFAULT,#{memName},#{memId},#{memPwd},#{memTel},#{memGender},#{memBirth},DEFAULT,DEFAULT,"Y",DEFAULT,#{updUser},#{mailYn},#{snsYn},#{memNick})
	</insert>
	
	<update id="updateTest" parameterType="com.themembers.common.dto.MemberDTO">
		UPDATE MEMBER
		<trim prefix="SET" suffixOverrides=",">
      		<if test="memName != null">memName = #{memName},</if>
      		<if test="memId != null">memId = #{memId},</if>
      		<if test="memPwd != null">memPwd = #{memPwd},</if>
      		<if test="memTel != null">memTel = #{memTel},</if>
      		<if test="memGender != null">memGender = #{memGender},</if>
      		<if test="memBirth != null">memBirth = #{memBirth},</if>
      		<if test="memType != null">memType = #{memType},</if>
      		<if test="regDate != null">regDate = #{regDate},</if>
      		<if test="useYn != null">useYn = #{useYn},</if>
      		<if test="updDate != null">updDate = #{updDate},</if>
      		<if test="updUser != null">updUser = #{updUser},</if>
      		<if test="mailYn != null">mailYn = #{mailYn},</if>
      		<if test="snsYn != null">snsYn = #{snsYn},</if>
      		<if test="memNick != null">memNick = #{memNick}</if>
   		</trim>
			WHERE memCode = #{memCode}
	</update>
	
	<delete id="deleteTest" parameterType="com.themembers.common.dto.MemberDTO">
		DELETE 
		FROM MEMBER 
		 WHERE memCode = #{memCode}
	</delete>
</mapper>
